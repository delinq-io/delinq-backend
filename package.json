{
  "name": "delinq.io-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "lint:fix": "eslint . --ext=.ts --fix",
    "migrations": "npm run migrations:rb && npm run migrations:run",
    "migrations:rb": "node ace migration:rollback",
    "migrations:run": "node ace migration:run",
    "seed": "node ace db:seed --files=database/seeders/index"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-adonis": "^1.3.1",
    "pino-pretty": "^4.8.0",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.4",
    "@adonisjs/core": "^5.1.7",
    "@adonisjs/lucid": "^17.0.1",
    "@adonisjs/mail": "^7.1.1",
    "@adonisjs/repl": "^3.1.2",
    "@adonisjs/session": "^6.0.3",
    "@adonisjs/view": "^6.0.2",
    "luxon": "^1.27.0",
    "on-finished": "^2.3.0",
    "pg": "^8.6.0",
    "phc-argon2": "^1.1.1",
    "pretty-ms": "^7.0.1",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  }
}
